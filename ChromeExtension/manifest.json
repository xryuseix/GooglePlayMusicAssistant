{
  "name": "GooglePlayMusicAssistant",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "GooglePlayMusicAssistant",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "js": ["jquery.min.js", "content_script.js"],
      "matches": ["https://play.google.com/music/listen#/all"]
    }
  ]
}
